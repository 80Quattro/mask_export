{
    "name": "ichhabrecht/mask-export",
    "description": "Export your mask elements as extension",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3 CMS",
        "Mask",
        "Export",
        "Content Elements",
        "Wizard"
    ],
    "homepage": "https://github.com/IchHabRecht/mask_export",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Nicole Cordes",
            "email": "typo3@cordes.de",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^7.6 || ^8.7",
        "typo3/cms-extbase": "^7.6 || ^8.7",
        "typo3/cms-fluid": "^7.6 || ^8.7",
        "mask/mask": "^2.0 || ^3.0"
    },
    "require-dev": {
        "typo3/cms-fluid-styled-content": "^7.6 || ^8.7",
        "nimut/testing-framework": "2.x-dev",
        "phpunit/phpunit": "^4.8"
    },
    "suggest": {
        "friendsofphp/php-cs-fixer": "Tool to automatically fix PHP coding standards issues",
        "phpstan/phpstan": "Use PHPStan for static PHP analysis"
    },
    "autoload": {
        "psr-4": {
            "IchHabRecht\\MaskExport\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IchHabRecht\\MaskExport\\Tests\\": "Tests/"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }
}
